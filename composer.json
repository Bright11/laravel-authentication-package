{
    "name": "brightweb/authentication",
    "version": "1.0.0",
  "description": "Advanced authentication package for Laravel 11+ and PHP 8+",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Brightweb\\Authentication\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Brightweb\\Authentication\\Providers\\BrightAuthProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Bright C Web",
            "email": "chikanwazuo@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^11.0"
    },
    "keywords": [
        "laravel",
        "laravel 11",
        "authentication",
        "php 8",
        "auth",
        "login",
        "user management",
        "oauth",
        "jwt",
        "api authentication",
        "brightweb"
    ],
    "minimum-stability": "stable",
    "support": {
        "issues": "https://github.com/Bright11/laravel-b-authentication-package/issues",
        "source": "https://github.com/Bright11/laravel-b-authentication-package"
    }
}
